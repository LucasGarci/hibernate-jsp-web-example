<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ClaseController</servlet-name>
        <servlet-class>controller.ClaseController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlumnoController</servlet-name>
        <servlet-class>controller.AlumnoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserController</servlet-name>
        <servlet-class>controller.UserController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MatriculaController</servlet-name>
        <servlet-class>controller.MatriculaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PersonalController</servlet-name>
        <servlet-class>controller.PersonalController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IngresosController</servlet-name>
        <servlet-class>controller.IngresosController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GastosController</servlet-name>
        <servlet-class>controller.GastosController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClaseController</servlet-name>
        <url-pattern>/ClaseController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlumnoController</servlet-name>
        <url-pattern>/AlumnoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserController</servlet-name>
        <url-pattern>/UserController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MatriculaController</servlet-name>
        <url-pattern>/MatriculaController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PersonalController</servlet-name>
        <url-pattern>/PersonalController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IngresosController</servlet-name>
        <url-pattern>/IngresosController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GastosController</servlet-name>
        <url-pattern>/GastosController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
